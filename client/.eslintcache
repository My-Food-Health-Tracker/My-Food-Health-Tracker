\n      {/* <DemoData /> */}\n      <BottomNavbar/>\n      </div>\n    )\n  }\n}\n","/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/AddFrequent.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/More.js",["184"],"import React, { Component } from 'react'\nimport { logout } from '../../services/auth';\nimport { Link } from 'react-router-dom';\nimport TopBar from '../shared/TopBar'\nimport BottomNavbar from '../shared/BottomNavbar'\nimport Icons from '../shared/Icons';\n\n\nconst handleLogout = props => {\n  console.log(props);\n  logout().then(() => {\n    props.setUser(null);\n  })\n}\n\nexport default function More(props) {\n  \n  return (\n    <div>\n      <TopBar icon=\"more\" title=\"More\"/>\n      <Link to='/' onClick={() => handleLogout(props)}><Icons icon=\"logout\"/>Logout</Link>\n      <BottomNavbar />\n    </div>\n  )\n}\n","/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/AddItem.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/add-entries/AddEnergy.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/add-entries/AddExercise.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/add-entries/AddSleep.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/shared/Calendar.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/shared/BottomNavBar.js",[],["185","186"],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/shared/EntryList.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/shared/TopBar.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/shared/Icons.js",[