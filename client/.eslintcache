r\"}}>\n          <div>\n            <div style={{\"display\": \"flex\", \"flexDirection\": \"row\", \"justifyContent\": \"center\", \"alignItems\": \"center\"}}>\n            <label htmlFor='name'  className=\"db fw4 lh-copy f6\">Name: </label>\n            <input\n              type='text'\n              id='name'\n              name='name'\n              onChange={this.handleChange}\n            />\n          </div>\n          <div style={{\"display\": \"flex\", \"flexDirection\": \"row\", \"justifyContent\": \"center\", \"alignItems\": \"center\"}}>\n            <label htmlFor='brand' className=\"db fw4 lh-copy f6\">Brand: </label>\n            <input \n              type='text'\n              id='brand'\n              name='brand'\n              value={this.state.brand}\n              onChange={this.handleChange}\n            />\n          </div>\n          <div style={{\"display\": \"flex\", \"flexDirection\": \"row\", \"justifyContent\": \"center\", \"alignItems\": \"center\"}}>\n          <label htmlFor='category' className=\"db fw4 lh-copy f6\">Category: </label>\n            <input \n              type='text'\n              id='category'\n              name='category'\n              value={this.state.category}\n              onChange={this.handleChange}\n            /> \n            </div>\n            </div>\n          <div>\n            <div>\n              <label htmlFor='servingAmount' className=\"db fw4 lh-copy f6\">Serving Amount: </label>\n              <input \n                type='number'\n                id='servingAmount'\n                name='servingAmount'\n                value={this.state.servingAmount}\n                onChange={this.handleChange}\n              />\n            </div>\n            <div>\n              <label htmlFor='servingSize' className=\"db fw4 lh-copy f6\">Serving Size: </label>\n              <input \n                type='text'\n                id='servingSize'\n                name='servingSize'\n                value={this.state.servingSize}\n                onChange={this.handleChange}\n              />\n              </div>\n              </div>\n          </div>\n        \n        <button type='submit' className=\"f6 link dim br-pill ba ph3 pv2 mb2 dib dark-blue\">Save</button>\n      </form>\n\n      <Link className=\"link blue hover-silver dib mh3 tc\" style={{\n            \"display\": \"flex\", \"flexDirection\":\"row\", \"justifyContent\": \"center\", \"alignItems\":\"center\"}}>\n          <Icons icon=\"FoodsDetails\"/>\n          <span className=\"f6 db\" style={{\"marginLeft\": \"10px\"}}>{this.state.ingredientCount} ingredients added</span>\n          </Link>\n      </div>\n    )\n}\n}","/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/Icons.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/BottomNavbar.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/bottomNavbar.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/Dashboard.js",["153","154"],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/Analysis.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/AddFrequent.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/More.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/nav-bar-options/AddItem.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/add-entries/AddEnergy.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/add-entries/AddExercise.js",[],"/Users/xiaomei/Desktop/ironhack/project-3/My-Food-Health-Tracker/client/src/components/add-entries/AddSymptoms.js",[],"/Users/xiaomei/De